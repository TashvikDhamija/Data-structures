#include <iostream>
using namespace std;

class BSTNode{
public:
	int data;
	BSTNode* left;
  BSTNode* right;
};
BSTNode* NewNode(int data){
  BSTNode* node= new BSTNode();
  node->data=data;
  node->left=NULL;
  node->right=NULL;
  return node;
}
BSTNode* InsertNode(BSTNode* root,int data){
  if(root==NULL){
    root=NewNode(data);
  }
  else if(root->data >= data){
    BSTNode* node = InsertNode(root->left,data);
  }
  else if(root->data <= data){
    BSTNode* node = InsertNode(root->right,data);
  }
  return root;
}
bool Search(BSTNode* root, int data){
  if(root==NULL){
    return false;
  }
  else if(root->data==data){
    return true;
  }
  BSTNode* temp = root;
  if(data<root->data){
    temp=root->left;
  }
  else{
    temp=root->right;
  }
  bool res=Search(temp,data);
  return res;
}



int main(){
  BSTNode* root = NULL;
  root = InsertNode(root,5);
  root = InsertNode(root,14);
  root = InsertNode(root,12);
  root = InsertNode(root,21);
  root = InsertNode(root,8);
  root = InsertNode(root,1);
  if(Search(root,5)==true){
      cout<<"T";
  }
  else{
      cout<<"F";
  }
  return 0;
}
